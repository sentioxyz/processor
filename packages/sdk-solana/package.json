{
  "name": "@sentio/sdk-solana",
  "license": "Apache-2.0",
  "version": "2.0.0-development",
  "scripts": {
    "compile": "tsc -p .",
    "build": "yarn gen_test && yarn compile",
    "build_all": "yarn lerna run --scope=@sentio/sdk-solana build --include-dependencies",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest",
    "gen_test": "ts-node-esm src/run-codegen.ts src/tests/abis src/tests/types"
  },
  "dependencies": {
    "@project-serum/anchor": "^0.26.0",
    "@sentio/sdk": "2.0.0-development",
    "@solana/web3.js": "^1.47.3",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@certusone/wormhole-sdk": "^0.9.8",
    "@types/lodash-es": "^4.17.6",
    "lodash-es": "^4.17.21"
  },
  "type": "module",
  "exports": {
    ".": "./lib/index.js",
    "./builtin": "./lib/builtin/index.js",
    "./codegen": "./lib/codegen/index.js"
  },
  "files": [
    "{lib,src}",
    "!{lib,src}/tests",
    "!**/*.test.{js,ts}"
  ],
  "typedoc": {
    "entryPoint": "./src/index.ts"
  }
}
